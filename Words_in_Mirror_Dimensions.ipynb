      "adoydE\n"
     ]
    }
   ],
   "source": [
    "word1=\"Edyoda\"\n",
    "word2=word1.split()\n",
    "rev_word1=[]\n",
    "\n",
    "for j in word2:\n",
    "    rev_word1.append(j[::-1])\n",
    "    word3 = \"\".join(rev_word1)\n",
    "print(word3)"
   ]
