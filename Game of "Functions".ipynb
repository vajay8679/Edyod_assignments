   ],
   "source": [
    "def sum_fun(num):\n",
    "    total = 0\n",
    "    for a in num:\n",
    "        total += a\n",
    "    return total\n",
    "        \n",
    "x=sum_fun((8, 2, 3, 0, 7))\n",
    "#y=sum_fun(x)\n",
    "print(x)"
